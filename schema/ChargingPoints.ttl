    skos:altLabel "IEC_62196 Type 2" ;
    foaf:depiction <https://upload.wikimedia.org/wikipedia/commons/f/f3/VDE-AR-E_2623-2-2-plug.jpg> .

mv:NEMA_14-30
    mv:ocmConnectionID "10" ;
    a mv:PlugType ;
    rdfs:comment "NEMA connectors are power plugs and receptacles used for AC mains electricity in North America and other countries that use the standards set by the US National Electrical Manufacturers Association. The NEMA 14 devices are four-wire grounding devices (hot-hot-neutral-ground) available in ratings from 15 to 60 A. The voltage rating is 250 V. Rated for 30A." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "NEMA 14-30"@en ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/NEMA_connector#NEMA_14> ;
